<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.base.mobile.version.MobileVersion" table="T_BASE_MOBILE_VERSION">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="identity"></generator>
		</id>
		<property name="mobile" column="MOBILE" />
		<property name="versionName" column="VERSION_NAME" />
		<property name="versionCode" column="VERSION_CODE" />
		<property name="description" column="DESCRIPTION" />
		<property name="releaseTime" column="RELEASE_TIME" />
		<property name="fileUrl" column="FILE_URL" />
		<property name="fileSize" column="FILE_SIZE" />
		<property name="remarks" column="REMARKS" />
		<property name="orderNumber" column="ORDER_NUMBER" />
		<property name="currentState" column="CURRENT_STATE" />
		<property name="isDelete" column="IS_DELETE" />
	    <property name="createTime" column="CREATE_TIME" />
		<property name="updateTime" column="UPDATE_TIME" />
	</class>
</hibernate-mapping>


